type User {
  id: ID! @id
  name: String!
  password: String!
  email: String @unique
  posts: [Post!]!
  tokens: [Token!]! @relation(onDelete: CASCADE)
}

type Token {
  id: ID! @id
  expiration: DateTime!
  user: User!
}

type Post {
  id: ID! @id
  title: String!
  published: Boolean! @default(value: false)
  author: User @relation(link: INLINE)
}